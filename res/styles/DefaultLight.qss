BaseTimelineView {
  qproperty-timelineBgColor: #F8F8F8;
  qproperty-timelineSeparatorColor: rgba(0, 0, 0, 20);
  qproperty-timelineRulerColor: #000000;
  qproperty-timelinePlayheadColor: rgb(206, 91, 87);
  qproperty-clipFillColor: #A5D6A7;
  qproperty-clipBorderColor: #FFB74D;
  qproperty-trackSelectedColor: #0078d4;
}

/* ======= BaseTracklistView 样式 (自定义属性) ======= */
BaseTracklistView {
    border: none;
    outline: 0;
    qproperty-trackBgColor: #F8F8F8;
    qproperty-trackColor: #F8F8F8;
    qproperty-trackSelectedColor: #0078d4;
    qproperty-trackHoverColor: #2a292f;
}
