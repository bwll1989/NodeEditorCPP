}

ads--CDockWidgetTab QPushButton:hover {
    background-color: #F5F5F5;
    color: #000000;
    border-color: #0078d4;

}

ads--CDockWidgetTab QPushButton:pressed {
    background-color: #0078d4;
    color: #FFFFFF;
}

ads--CAutoHideTab {
